{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "Set up of Vue.js template with webpack",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "dev": "webpack-dev-server --hot --env dev",
    "build:dev": "webpack --env dev --progress --profile --colors",
    "build:prod": "webpack --env prod --progress --profile --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nate-stoltz/vue-template.git"
  },
  "author": "Nate Stoltz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nate-stoltz/vue-template/issues"
  },
  "homepage": "https://github.com/nate-stoltz/vue-template#readme",
  "dependencies": {
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.11",
    "vue-page-transition": "^0.2.2",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.1",
    "eslint-plugin-vue-libs": "^4.0.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.0",
    "vue-router": "^3.1.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
